{
  "name": "social-network",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/social-network/src",
  "prefix": "web",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/social-network",
        "index": "apps/social-network/src/index.html",
        "main": "apps/social-network/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/social-network/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/social-network/src/favicon.ico",
          "apps/social-network/src/assets"
        ],
        "styles": [
          "apps/social-network/src/styles.scss",
          "libs/ui-elements/src/assets/scss/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "social-network:build:production"
        },
        "development": {
          "browserTarget": "social-network:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "social-network:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/social-network/**/*.ts",
          "apps/social-network/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/social-network/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
